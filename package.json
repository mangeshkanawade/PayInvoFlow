{
  "name": "payinvoflow",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build": "ng build --configuration production",
    "postbuild": "copyfiles -u 1 api/index.js dist/payinvoflow/api",
    "vercel-build": "npm run build",
    "deploy": "npm run build && vercel --prod",
    "serve:ssr:payinvoflow": "node dist/payinvoflow/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.4",
    "@angular/cdk": "^20.0.3",
    "@angular/cli": "^20.0.3",
    "@angular/common": "^20.0.4",
    "@angular/compiler": "^20.0.4",
    "@angular/core": "^20.0.4",
    "@angular/forms": "^20.0.4",
    "@angular/platform-browser": "^20.0.4",
    "@angular/platform-browser-dynamic": "^20.0.4",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.0.4",
    "@angular/ssr": "^20.0.4",
    "@primeuix/themes": "latest",
    "@types/jasmine": "4.3.1",
    "@types/node": "22.9.0",
    "chart.js": "4.4.2",
    "express": "^5.1.0",
    "primeicons": "latest",
    "primeng": "20.1.1",
    "quill": "2.0.2",
    "rxjs": "~7.8.2",
    "tailwindcss": "^3",
    "tailwindcss-primeui": "latest",
    "tslib": "^2.5.0",
    "zone.js": "~0.15.0",
    "typescript": "~5.8.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.3",
    "@angular/build": "^20.3.1",
    "@angular/cli": "^20.0.3",
    "@angular/compiler-cli": "^20.0.3",
    "@angular/language-service": "^20.0.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~4.3.1",
    "@types/node": "^22.9.0",
    "@vercel/node": "^5.3.22",
    "autoprefixer": "^10.4.20",
    "codelyzer": "^0.0.28",
    "copyfiles": "^2.4.1",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "postcss": "^8.4.41",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.10",
    "ts-node": "~10.9.2",
    "typescript": "~5.8.3"
  }
}